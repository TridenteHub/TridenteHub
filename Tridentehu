-- LocalScript dentro de StarterGui ou PlayerGui

-- Servi√ßos
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local Lighting = game:GetService("Lighting")
local Workspace = game:GetService("Workspace")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

player.CharacterAdded:Connect(function(char)
	character = char
	hrp = char:WaitForChild("HumanoidRootPart")
	humanoid = char:WaitForChild("Humanoid")
end)

-- Configura√ß√µes
local originalFogEnd = Lighting.FogEnd

-- Anti Kick
pcall(function()
	local mt = getrawmetatable(game)
	if mt then
		local oldNamecall = mt.__namecall
		setreadonly(mt, false)
		mt.__namecall = newcclosure(function(self, ...)
			local method = getnamecallmethod()
			if method == "Kick" then return end
			return oldNamecall(self, ...)
		end)
		setreadonly(mt, true)
	end
end)

-- Tela de Key
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "TRIDENTE_HUB_GUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

local keyFrame = Instance.new("Frame")
keyFrame.Size = UDim2.new(0, 360, 0, 240)
keyFrame.Position = UDim2.new(0.5, -180, 0.5, -120)
keyFrame.BackgroundColor3 = Color3.fromRGB(20,20,25)
keyFrame.BorderSizePixel = 0
keyFrame.Active = true
keyFrame.Draggable = true
keyFrame.Parent = screenGui

Instance.new("UICorner", keyFrame).CornerRadius = UDim.new(0,15)

local title = Instance.new("TextLabel", keyFrame)
title.Size = UDim2.new(1,0,0,50)
title.BackgroundTransparency = 1
title.Text = "üî± TRIDENTE HUB KEY üî±"
title.TextColor3 = Color3.fromRGB(0,170,255)
title.Font = Enum.Font.Arcade
title.TextSize = 22

local keyBox = Instance.new("TextBox", keyFrame)
keyBox.Size = UDim2.new(0,280,0,40)
keyBox.Position = UDim2.new(0.5,-140,0.5,-20)
keyBox.PlaceholderText = "Insira sua key..."
keyBox.Text = ""
keyBox.Font = Enum.Font.Arcade
keyBox.TextSize = 18
keyBox.TextColor3 = Color3.fromRGB(255,255,255)
keyBox.BackgroundColor3 = Color3.fromRGB(35,35,40)
Instance.new("UICorner", keyBox).CornerRadius = UDim.new(0,10)

local verifyBtn = Instance.new("TextButton", keyFrame)
verifyBtn.Size = UDim2.new(0,280,0,40)
verifyBtn.Position = UDim2.new(0.5,-140,0.5,40)
verifyBtn.BackgroundColor3 = Color3.fromRGB(0,120,255)
verifyBtn.Font = Enum.Font.Arcade
verifyBtn.Text = "Verificar Key"
verifyBtn.TextSize = 18
verifyBtn.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", verifyBtn).CornerRadius = UDim.new(0,10)

local closeBtn = Instance.new("TextButton", keyFrame)
closeBtn.Size = UDim2.new(0,30,0,30)
closeBtn.Position = UDim2.new(1,-35,0,5)
closeBtn.BackgroundColor3 = Color3.fromRGB(255,50,50)
closeBtn.Text = "‚ùå"
closeBtn.TextColor3 = Color3.fromRGB(255,255,255)
closeBtn.Font = Enum.Font.Arcade
closeBtn.TextSize = 18
Instance.new("UICorner", closeBtn).CornerRadius = UDim.new(0,5)

closeBtn.MouseButton1Click:Connect(function()
	keyFrame:Destroy()
end)

local getKeyLink = Instance.new("TextLabel", keyFrame)
getKeyLink.Size = UDim2.new(1,0,0,30)
getKeyLink.Position = UDim2.new(0,0,1,-35)
getKeyLink.BackgroundTransparency = 1
getKeyLink.Text = "üîó Pegar key: https://rekonise.com/tridente-hub-key-v1-4bop2 üîó"
getKeyLink.Font = Enum.Font.Arcade
getKeyLink.TextColor3 = Color3.fromRGB(180,180,255)
getKeyLink.TextSize = 14

local correctKey = "KeynovaRoubeumBrainrot"

verifyBtn.MouseButton1Click:Connect(function()
	if keyBox.Text == correctKey then
		verifyBtn.Text = "‚úÖ Key V√°lida!"
		verifyBtn.BackgroundColor3 = Color3.fromRGB(0,255,128)
		task.wait(1)
		keyFrame:Destroy()
	else
		verifyBtn.Text = "‚ùå Key Inv√°lida!"
		verifyBtn.BackgroundColor3 = Color3.fromRGB(255,50,50)
		task.wait(1.2)
		verifyBtn.Text = "Verificar Key"
		verifyBtn.BackgroundColor3 = Color3.fromRGB(0,120,255)
	end
end)

repeat task.wait() until not keyFrame or not keyFrame.Parent

-- Painel Principal
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0,320,0,460)
mainFrame.Position = UDim2.new(0.3,0,0.2,0)
mainFrame.BackgroundColor3 = Color3.fromRGB(18,18,20)
mainFrame.BorderSizePixel = 0
mainFrame.Parent = screenGui
mainFrame.Active = true
mainFrame.Draggable = true

Instance.new("UICorner", mainFrame).CornerRadius = UDim.new(0,18)
local stroke = Instance.new("UIStroke", mainFrame)
stroke.Thickness = 3
stroke.Color = Color3.fromRGB(0,128,255)

local titleMain = Instance.new("TextLabel", mainFrame)
titleMain.Size = UDim2.new(1,-20,0,44)
titleMain.Position = UDim2.new(0,10,0,8)
titleMain.BackgroundTransparency = 1
titleMain.Text = "TRIDENTE HUB (V1)"
titleMain.TextColor3 = Color3.fromRGB(160,210,255)
titleMain.Font = Enum.Font.Arcade
titleMain.TextSize = 22
titleMain.TextXAlignment = Enum.TextXAlignment.Left

-- üîΩ Bot√£o de minimizar com anima√ß√£o e logo do drag√£o
local minimizeBtn = Instance.new("TextButton", mainFrame)
minimizeBtn.Size = UDim2.new(0,30,0,30)
minimizeBtn.Position = UDim2.new(1,-35,0,5)
minimizeBtn.BackgroundColor3 = Color3.fromRGB(255,180,0)
minimizeBtn.Text = "‚ûñ"
minimizeBtn.TextColor3 = Color3.fromRGB(255,255,255)
minimizeBtn.Font = Enum.Font.Arcade
minimizeBtn.TextSize = 18
Instance.new("UICorner", minimizeBtn).CornerRadius = UDim.new(0,5)

-- üêâ Logo do drag√£o para reabrir
local reopenLogo = Instance.new("ImageButton")
reopenLogo.Size = UDim2.new(0,100,0,100)
reopenLogo.Position = UDim2.new(1,-110,1,-110)
reopenLogo.BackgroundTransparency = 1
reopenLogo.ImageTransparency = 1
reopenLogo.Image = "rbxassetid://120877700350721"
reopenLogo.Visible = false
reopenLogo.Parent = screenGui
reopenLogo.Active = true
reopenLogo.Draggable = true

-- Fun√ß√µes de anima√ß√£o
local function fadeInLogo()
	reopenLogo.Visible = true
	TweenService:Create(reopenLogo, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {ImageTransparency = 0}):Play()
end

local function fadeOutLogo()
	local tween = TweenService:Create(reopenLogo, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {ImageTransparency = 1})
	tween:Play()
	tween.Completed:Connect(function()
		reopenLogo.Visible = false
	end)
end

local function fadeOutMain()
	local tween = TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {BackgroundTransparency = 1})
	for _, v in pairs(mainFrame:GetDescendants()) do
		if v:IsA("TextLabel") or v:IsA("TextButton") or v:IsA("TextBox") then
			TweenService:Create(v, TweenInfo.new(0.4), {TextTransparency = 1, BackgroundTransparency = 1}):Play()
		end
	end
	tween:Play()
	tween.Completed:Connect(function()
		mainFrame.Visible = false
		fadeInLogo()
	end)
end

local function fadeInMain()
	mainFrame.Visible = true
	local tween = TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0})
	for _, v in pairs(mainFrame:GetDescendants()) do
		if v:IsA("TextLabel") or v:IsA("TextButton") or v:IsA("TextBox") then
			TweenService:Create(v, TweenInfo.new(0.4), {TextTransparency = 0, BackgroundTransparency = 0}):Play()
		end
	end
	tween:Play()
	fadeOutLogo()
end

minimizeBtn.MouseButton1Click:Connect(fadeOutMain)
reopenLogo.MouseButton1Click:Connect(fadeInMain)

-- Aqui continuaria sua cria√ß√£o dos bot√µes do painel (createButton etc.)
